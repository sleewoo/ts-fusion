{
  "type": "module",
  "name": "ts-fusion",
  "version": "0.0.2",
  "nodeVersion": "22.14.0",
  "packageManager": "pnpm@10.8.0",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": ["pkg/*"],
  "exports": {
    ".": {
      "types": "./pkg/index.d.ts",
      "default": "./pkg/index.js"
    }
  },
  "scripts": {
    "// ✨ build": "",
    "build:tests": "file-bundler -c test/file-bundler.toml",
    "build": "esbuilder ./src/index -o pkg -l .hbs=text -s ./scripts/build",
    "// ✨ test": "",
    "test": "vitest run",
    "// ✨ publish": "",
    "prepub": "./scripts/publish/prepare",
    "pub": "changeset publish"
  },
  "dependencies": {
    "crc": "^4.3.2",
    "ts-morph": "^25.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.2",
    "@libconfig/biome": "^0.0.1",
    "@libconfig/typescript": "^0.0.1",
    "@libutil/esbuilder": "^0.0.7",
    "@libutil/file-bundler": "^0.0.6",
    "@types/node": "^22.14.1",
    "resolve-tspaths": "^0.8.23",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
